{
  "expo": {
    "name": "tracking-client",
    "slug": "tracking-client",
    "sdkVersion": "37.0.0",
    "ios": {
      "bundleIdentifier": "com.fnaranjoec.tracking.client"
    },
    "android": {
      "package": "com.fnaranjoec.tracking.client",
      "config": {
        "googleMaps": { "apiKey": "xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx" }
      }
    },
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
